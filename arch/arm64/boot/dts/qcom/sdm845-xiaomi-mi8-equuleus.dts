/dts-v1/;

#include "sdm845-xiaomi-mi8-common.dtsi"

/ {
	model = "Xiaomi Technologies, Inc. Equuleus MP v2.1";
	compatible = "xiaomi,equuleus", "qcom,sdm845", "qcom,mtp", "qcom,sdm845-mtp",;
	
	/* required for bootloader to select correct board */
	// REVIEW(Krey): https://github.com/LineageOS/android_kernel_xiaomi_sdm845/blob/lineage-21/arch/arm64/boot/dts/qcom/equuleus-mp-v2.1.dts#L25
	qcom,board-id = <79 0>;

	// REVIEW(Krey): Taken from 5.12.8 port
	/* Regulators */
	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};

	// REVIEW(Krey): Taken from 5.12.8 port
	/*
	 * Apparently RPMh does not provide support for PM8998 S4 because it
	 * is always-on; model it as a fixed regulator.
	 */
	vreg_s4a_1p8: pm8998-smps4 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;

		regulator-boot-on;
		vin-supply = <&vph_pwr>;
	};
};

