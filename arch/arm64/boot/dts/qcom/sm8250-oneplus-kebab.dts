#include "sm8250-oneplus-common.dtsi"

/ {
	model = "OnePlus 8T";
	compatible = "oneplus,kebab", "qcom,sm8250";
};

&display_panel {
	compatible = "samsung,amb655x";
	status = "okay";
};

&i2c13 {
	clock-frequency = <400000>;
	status = "okay";

	/* Syanptics S3908 touchscreen @ 4b */
};
